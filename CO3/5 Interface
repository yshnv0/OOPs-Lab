import java.util.Scanner;

public class Result implements Student,Sports{
	
	void display(){	
		System.out.println("Name: "+name+"\nMark for Java: "+java+"\nMark for Sports: "+sm);
	}
	
	public static void main(String[] args){
		Result obj=new Result();
		obj.display();
			
	}
}

interface Student{	
	String name="Human";
	int java=100;
	
}


interface Sports{
	int sm=30;
	

}
